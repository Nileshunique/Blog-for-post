<%- include("common/header"); -%>
    <h1>Home</h1>
    <p>
        <%= startingContent %>
    </p>
    <% posts.forEach(function (post) { %>
        <h3>
            <%= post.postTitle %>
        </h3>
        <p>
            <%= post.postContent.substring(0, 100)+"..." %>
                <a href="/posts/<%= post.id %>">Read More</a>
        </p>
        <% }) %>
            <%- include("common/footer"); -%>